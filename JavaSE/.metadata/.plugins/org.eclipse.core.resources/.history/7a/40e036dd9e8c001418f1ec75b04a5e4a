class Resource{
	int id;
	String name;
	
}

//产线
class Input implements Runnable{
	Resource r;
	
	public Input(Resource r) {
		super();
		this.r = r;
	}
	int key = 0;
	public void run() {
		
		
	}
	
}
//销售
class Output implements Runnable{
	Resource r;
	public Output(Resource r) {
		super();
		this.r = r;
	}
	@Override
	public void run() {
		
	}
	
}
