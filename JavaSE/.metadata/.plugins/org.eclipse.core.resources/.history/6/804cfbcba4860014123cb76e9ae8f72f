package day12;

import java.util.LinkedList;
import java.util.List;



public class LinkedListHome {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		LinkedList arr = new LinkedList();
		
		arr.add("13913535174");
		//arr.add("1321");
		
		Integer a = new Integer(arr.size());
		arr.toArray(a);
		System.out.println(a[0]);
	}
}
