public class Problem08 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.println("请输入a的值：");
		int n = in.nextInt();
		System.out.println("请输入相加的个数：");
		int m = in.nextInt();
	}

}
