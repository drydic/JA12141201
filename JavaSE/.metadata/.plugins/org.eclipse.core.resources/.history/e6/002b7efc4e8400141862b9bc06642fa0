package practice15;

class Person1{
	private String name;
	private String idStr;
	public Person1() {}
	public Person1(String name, String idStr) {
		this.name = name;
		this.idStr = idStr;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getIdStr() {
		return idStr;
	}
	public void setIdStr(String idStr) {
		this.idStr = idStr;
	}
	public boolean equals(Object obj){
		if (this == obj) {
			return true;
		}
		if (obj!=null&&obj.getClass() == Person1.class) {
			Person1 personObjPerson1 = (Person1)obj;
			if (this.getIdStr().equals(personObjPerson1.getIdStr())) {
				return true;
			}
		}
		return false;
	}
}
class Dog{}


public class OverrideEqualsError {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Person1 person1 = new Person1();
		System.out.println("Person1对象是否equals Dog对象？" + person1.equals(new Dog()));
		System.out.println("Person对象是否equals String对象？" + person1.equals(new String("Hello")));
	}

}
