package list;

import java.util.ArrayList;

import bean.Person;

public class ArrayListDemo2 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ArrayList al = new ArrayList();
		
		for (int i = 0; i < 5; i++) {
			al.add(new Person(i,"it-" + i))
		}
		
	}

}
