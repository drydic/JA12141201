
class Monkey{
	String name;

	public Monkey(String name) {
		super();
		this.name = name;
	}
	
	public void fight(){
		for (int i = 1; i < 11; i++) {
			System.out.println(name+"打败第"+i+"个天兵天将。");
		}
	}
}
public class NotThread {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Monkey m1 = new Monkey("第一个小悟空");
	}

}
