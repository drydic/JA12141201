package file;

import java.io.FileReader;
import java.io.IOException;

public class ReaderDemo {

	
	public static void main(String[] args) throws IOException {
		FileReader fr = new FileReader("demo.txt");
		int ch = 0;
		while((ch=fr.read()) != -1){
			System.out.print(ch);
		}
		
		
		fr.close();
	}

}
