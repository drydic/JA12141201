package priority;
class Demo extends Thread{
	String name;

	public Demo(String name) {
		super();
		this.name = name;
	}
	public void run() {
		super.run();
		System.out.println(getName());
	}
}
public class PriorityDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Demo d1 = new Demo("线程1");
		Demo d2 = new Demo("线程2");
		Demo d3 = new Demo("线程3");

	}

}
